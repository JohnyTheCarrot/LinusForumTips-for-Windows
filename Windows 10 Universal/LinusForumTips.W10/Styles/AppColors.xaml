<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <Color x:Key="SectionTitleForegroundColor">#ffffff</Color>

            <Color x:Key="TitleForegroundColor">#ffffff</Color>
            <Color x:Key="SubtitleForegroundColor">#ffffff</Color>

            <Color x:Key="DescriptionForegroundColor">#ffffff</Color>

            <Color x:Key="NavigationPaneBackgroundColor">#f7630c</Color>
            <Color x:Key="NavigationPaneTextColor">#ffffff</Color>

            <Color x:Key="NavigationPaneButtonColor">#f7630c</Color>
            <Color x:Key="NavigationPaneButtonForegroundColor">#ffffff</Color>

            <Color x:Key="AppBarBackgroundColor">#f7630c</Color>
            <Color x:Key="AppBarForegroundColor">#ffffff</Color>

            <Color x:Key="BoxBackgroundColor">#f7630c</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
